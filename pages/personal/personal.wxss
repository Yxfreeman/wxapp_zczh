page{background-color: #fff;}
.warp{padding: 0 30rpx;font-family: "微软雅黑";color: #000;font-size: 30rpx;}
.userPic {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 30rpx;
}
.userPic image {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}
.userPic text{
  color: #aaa;
}
